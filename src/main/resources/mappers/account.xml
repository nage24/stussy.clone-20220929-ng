<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "https://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.stussy.stussy.clone20220929ng.repository.AccountRepository">

    <!-- insert 두번 하려면 yml 설정 : url 뒤로 ?allowMultiQueries=true 를 준다 ... -->
    <insert id="save" parameterType="com.stussy.stussy.clone20220929ng.dto.User">
        insert into
        user_mst
        values(
        0,
        #{username},
        #{oauth_username},
        #{password},
        #{name},
        #{email},
        #{provider},
        1,
        now(),
        now()
        )
    </insert>


</mapper>